{
    "name": "quynp/lazada",
    "description": "Lazada SDK by Quy Nguyen",
    "authors": [
        {
            "name": "Quy Nguyen",
            "email": "quynp@vutatech.vn"
        }
    ],
    "type": "library",
    "keywords": [
      "lazada",
      "openapi"
    ],
    "homepage": "https://github.com/quynp/lazada",
    "license": "BSD-3-Clause",
    "support": {
      "issues": "https://github.com/quynp/lazada/issues"
    },
    "require": {
      "php": ">=7.1",
      "ext-json": "*",
      "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
      "phpunit/phpunit": "^7.0",
      "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
      "psr-4": {
        "Lazada\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Lazada\\Tests\\": "tests"
      }
    },
    "config": {
      "sort-packages": true
    },
    "scripts": {
      "check": [
        "@cs-check",
        "@test"
      ],
      "cs-check": "phpcs",
      "cs-fix": "phpcbf",
      "test": "phpunit",
      "coverage-html": "phpunit --coverage-html=build/coverage-html"
    }
  }
  